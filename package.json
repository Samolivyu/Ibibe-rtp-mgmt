{
  "name": "merge",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Combined project for RTP calculation, validation, and API integration for gaming applications.",
  "main": "api/src/app.js",
  "scripts": {
    "start": "node api/src/app.js",
    "test": "npm run test:rtp",
    "test:rtp": "npx playwright test tests/rtp-test.spec.js",
    "test:rtp:headed": "npx playwright test tests/rtp-test.spec.js --headed",
    "test:rtp:debug": "npx playwright test tests/rtp-test.spec.js --debug",
    "report:rtp": "npx playwright show-report reports/html",
    "generate:sample-data": "node rtp/scripts/index.js generate-sample-data",
    "postinstall": "mkdir -p data reports/html reports/json reports/junit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "axios-rate-limit": "^1.4.0",
    "chalk": "^4.1.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "js-yaml": "^4.1.0",
    "mathjs": "^14.5.2",
    "node-fetch": "^2.7.0",
    "playwright": "^1.53.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@types/node": "^24.0.3",
    "concurrently": "^8.2.2"
  }
}